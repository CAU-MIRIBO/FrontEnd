{
    "action": {
        "default_icon": {
            "128": "/img/icons/128x128.png",
            "16": "/img/icons/16x16.png",
            "32": "/img/icons/32x32.png",
            "48": "/img/icons/48x48.png"
        },
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "css": [ "/lib/jquery-ui/jquery-ui.min.css", "main.css" ],
            "js": [ "/lib/html2canvas/html2canvas.min.js", "/lib/jquery/jquery.min.js", "/lib/jquery-ui/jquery-ui.min.js", "main.js" ],
            "matches": [ "http://*/*", "https://*/*" ]
        }
    ],
    "icons": {
        "128": "/img/icons/128x128.png",
        "16": "/img/icons/16x16.png",
        "32": "/img/icons/32x32.png",
        "48": "/img/icons/48x48.png"
    },
    "key": "",
    "manifest_version": 3,
    "name": "MIRIBO",
    "permissions": [ "storage", "activeTab" ],

    "version": "1.0",
    "web_accessible_resources": [
        {
            "matches": [ "http://*/*", "https://*/*" ],
            "resources": [ "/img/status/red.png", "/img/status/yellow.png", "/img/status/green.png", "/img/status/normal.png", "link-previewer-box.html" ]
        }
    ]
}
